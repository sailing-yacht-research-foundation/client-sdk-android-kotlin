<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.syrf.testapp">

    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.TestApp">
        <activity
            android:name=".activities.NavigationTestActivity"
            android:exported="false" />

        <meta-data
            android:name="io.syrf.sdk.API_KEY"
            android:value="@string/syrf_api_key" />

        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activities.LocationActivity"
            android:label="@string/location" />
        <activity
            android:name=".activities.AcceleroSensorActivity"
            android:label="@string/accelerometer_sensor" />
        <activity
            android:name=".activities.MagneticSensorActivity"
            android:label="@string/magnetic_sensor" />
        <activity
            android:name=".activities.GyroscopeSensorActivity"
            android:label="@string/gyroscope_sensor" />
        <activity
            android:name=".activities.FirstSampleActivity"
            android:label="@string/first_sample" />
        <activity
            android:name=".activities.SecondSampleActivity"
            android:label="@string/second_sample" />
    </application>

</manifest>